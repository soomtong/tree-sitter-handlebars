{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "handlebars_html",
  "rules": {
    "source_file": {
      "type": "REPEAT1",
      "content": {
        "type": "SYMBOL",
        "name": "_content"
      }
    },
    "_content": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "literal_text"
        },
        {
          "type": "SYMBOL",
          "name": "_handlebars"
        },
        {
          "type": "SYMBOL",
          "name": "raw_handlebars"
        },
        {
          "type": "SYMBOL",
          "name": "handleblock"
        },
        {
          "type": "SYMBOL",
          "name": "decorator_block"
        },
        {
          "type": "SYMBOL",
          "name": "partial"
        },
        {
          "type": "SYMBOL",
          "name": "partial_block"
        },
        {
          "type": "SYMBOL",
          "name": "simple_comment"
        },
        {
          "type": "SYMBOL",
          "name": "block_comment"
        }
      ]
    },
    "literal_text": {
      "type": "PATTERN",
      "value": "[^{]+"
    },
    "_handlebars": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "{{"
        },
        {
          "type": "SYMBOL",
          "name": "_expression"
        },
        {
          "type": "STRING",
          "value": "}}"
        }
      ]
    },
    "raw_handlebars": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "{{{"
        },
        {
          "type": "SYMBOL",
          "name": "_expression"
        },
        {
          "type": "STRING",
          "value": "}}}"
        }
      ]
    },
    "_identifier": {
      "type": "CHOICE",
      "members": [
        {
          "type": "PATTERN",
          "value": "[a-zA-Z0-9_-]+"
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "@"
            },
            {
              "type": "SYMBOL",
              "name": "_special_identifier"
            }
          ]
        }
      ]
    },
    "_special_identifier": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "partial-block"
        },
        {
          "type": "STRING",
          "value": "index"
        }
      ]
    },
    "_template_name": {
      "type": "FIELD",
      "name": "template",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SYMBOL",
            "name": "_identifier"
          },
          {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": "("
              },
              {
                "type": "SYMBOL",
                "name": "_expression"
              },
              {
                "type": "STRING",
                "value": ")"
              }
            ]
          }
        ]
      }
    },
    "partial": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "{{"
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": ">"
            },
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "_template_name"
                },
                {
                  "type": "CHOICE",
                  "members": [
                    {
                      "type": "SYMBOL",
                      "name": "_expression"
                    },
                    {
                      "type": "BLANK"
                    }
                  ]
                },
                {
                  "type": "REPEAT",
                  "content": {
                    "type": "SYMBOL",
                    "name": "context_injection"
                  }
                }
              ]
            }
          ]
        },
        {
          "type": "STRING",
          "value": "}}"
        }
      ]
    },
    "simple_comment": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "{{!"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "comment_content"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "STRING",
          "value": "}}"
        }
      ]
    },
    "block_comment": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "{{!--"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "comment_content"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "STRING",
          "value": "--}}"
        }
      ]
    },
    "comment_content": {
      "type": "PATTERN",
      "value": "[^-}]+"
    },
    "_expression": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "helper_call"
        },
        {
          "type": "SYMBOL",
          "name": "context_reference"
        },
        {
          "type": "SYMBOL",
          "name": "string"
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "("
            },
            {
              "type": "SYMBOL",
              "name": "_expression"
            },
            {
              "type": "STRING",
              "value": ")"
            }
          ]
        }
      ]
    },
    "string": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "'"
            },
            {
              "type": "PATTERN",
              "value": "[^']*"
            },
            {
              "type": "STRING",
              "value": "'"
            }
          ]
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "\""
            },
            {
              "type": "PATTERN",
              "value": "[^\"]*"
            },
            {
              "type": "STRING",
              "value": "\""
            }
          ]
        }
      ]
    },
    "_field_path": {
      "type": "PREC",
      "value": 1,
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SEQ",
            "members": [
              {
                "type": "SYMBOL",
                "name": "_identifier"
              },
              {
                "type": "CHOICE",
                "members": [
                  {
                    "type": "SEQ",
                    "members": [
                      {
                        "type": "PATTERN",
                        "value": "[./]"
                      },
                      {
                        "type": "SYMBOL",
                        "name": "_field_path"
                      }
                    ]
                  },
                  {
                    "type": "BLANK"
                  }
                ]
              }
            ]
          },
          {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": ".."
              },
              {
                "type": "CHOICE",
                "members": [
                  {
                    "type": "SEQ",
                    "members": [
                      {
                        "type": "STRING",
                        "value": "/"
                      },
                      {
                        "type": "SYMBOL",
                        "name": "_field_path"
                      }
                    ]
                  },
                  {
                    "type": "BLANK"
                  }
                ]
              }
            ]
          }
        ]
      }
    },
    "context_reference": {
      "type": "SYMBOL",
      "name": "_field_path"
    },
    "helper_call": {
      "type": "PREC_LEFT",
      "value": 0,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "FIELD",
            "name": "helper",
            "content": {
              "type": "SYMBOL",
              "name": "_identifier"
            }
          },
          {
            "type": "REPEAT1",
            "content": {
              "type": "SYMBOL",
              "name": "_expression"
            }
          }
        ]
      }
    },
    "partial_block": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_partial_start"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "_content"
          }
        },
        {
          "type": "SYMBOL",
          "name": "_block_end"
        }
      ]
    },
    "decorator_block": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_decorator_start"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "_content"
          }
        },
        {
          "type": "SYMBOL",
          "name": "_block_end"
        }
      ]
    },
    "handleblock": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_block_start"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "_content"
          }
        },
        {
          "type": "SYMBOL",
          "name": "_block_end"
        }
      ]
    },
    "_block_header": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "block_helper",
          "content": {
            "type": "SYMBOL",
            "name": "_push_tag"
          }
        },
        {
          "type": "FIELD",
          "name": "block_arguments",
          "content": {
            "type": "REPEAT",
            "content": {
              "type": "SYMBOL",
              "name": "_expression"
            }
          }
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "FIELD",
            "name": "context_injection",
            "content": {
              "type": "SYMBOL",
              "name": "context_injection"
            }
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "FIELD",
              "name": "context_binding",
              "content": {
                "type": "SYMBOL",
                "name": "context_binding"
              }
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "context_injection": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_identifier"
        },
        {
          "type": "STRING",
          "value": "="
        },
        {
          "type": "SYMBOL",
          "name": "_expression"
        }
      ]
    },
    "context_binding": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "as"
        },
        {
          "type": "STRING",
          "value": "|"
        },
        {
          "type": "SYMBOL",
          "name": "_identifier"
        },
        {
          "type": "STRING",
          "value": "|"
        }
      ]
    },
    "_block_start": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "{{"
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "#"
            },
            {
              "type": "SYMBOL",
              "name": "_block_header"
            }
          ]
        },
        {
          "type": "STRING",
          "value": "}}"
        }
      ]
    },
    "_block_end": {
      "type": "ALIAS",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "{{"
          },
          {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": "/"
              },
              {
                "type": "SYMBOL",
                "name": "_pop_tag"
              }
            ]
          },
          {
            "type": "STRING",
            "value": "}}"
          }
        ]
      },
      "named": false,
      "value": "/block"
    },
    "_decorator_start": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "{{"
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "TOKEN",
              "content": {
                "type": "SEQ",
                "members": [
                  {
                    "type": "STRING",
                    "value": "#"
                  },
                  {
                    "type": "STRING",
                    "value": "*"
                  }
                ]
              }
            },
            {
              "type": "SYMBOL",
              "name": "_block_header"
            }
          ]
        },
        {
          "type": "STRING",
          "value": "}}"
        }
      ]
    },
    "_partial_start": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "{{"
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "TOKEN",
              "content": {
                "type": "SEQ",
                "members": [
                  {
                    "type": "STRING",
                    "value": "#"
                  },
                  {
                    "type": "STRING",
                    "value": ">"
                  }
                ]
              }
            },
            {
              "type": "SYMBOL",
              "name": "_block_header"
            }
          ]
        },
        {
          "type": "STRING",
          "value": "}}"
        }
      ]
    },
    "_push_tag": {
      "type": "SYMBOL",
      "name": "_identifier"
    },
    "_pop_tag": {
      "type": "SYMBOL",
      "name": "_identifier"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}