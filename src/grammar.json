{
  "name": "handlebars_html",
  "rules": {
    "source_file": {
      "type": "REPEAT1",
      "content": {
        "type": "SYMBOL",
        "name": "_content"
      }
    },
    "_content": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "literal_text"
        },
        {
          "type": "SYMBOL",
          "name": "_handlebars"
        },
        {
          "type": "SYMBOL",
          "name": "handleblock"
        },
        {
          "type": "SYMBOL",
          "name": "macro"
        },
        {
          "type": "SYMBOL",
          "name": "partial"
        }
      ]
    },
    "literal_text": {
      "type": "PATTERN",
      "value": "[^{]+"
    },
    "_handlebars": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "{{"
        },
        {
          "type": "SYMBOL",
          "name": "_expression"
        },
        {
          "type": "STRING",
          "value": "}}"
        }
      ]
    },
    "_expression": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "helper_call"
        },
        {
          "type": "SYMBOL",
          "name": "context_reference"
        }
      ]
    },
    "_identifier": {
      "type": "PATTERN",
      "value": "[a-zA-Z]+"
    },
    "context_reference": {
      "type": "PREC",
      "value": 1,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "SYMBOL",
            "name": "_identifier"
          },
          {
            "type": "REPEAT",
            "content": {
              "type": "SEQ",
              "members": [
                {
                  "type": "STRING",
                  "value": "."
                },
                {
                  "type": "SYMBOL",
                  "name": "_identifier"
                }
              ]
            }
          }
        ]
      }
    },
    "helper_call": {
      "type": "PREC_LEFT",
      "value": 0,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "FIELD",
            "name": "helper",
            "content": {
              "type": "SYMBOL",
              "name": "_identifier"
            }
          },
          {
            "type": "REPEAT1",
            "content": {
              "type": "SYMBOL",
              "name": "_expression"
            }
          }
        ]
      }
    },
    "_block_start": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "{{"
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "#"
            },
            {
              "type": "FIELD",
              "name": "block_helper",
              "content": {
                "type": "SYMBOL",
                "name": "_expression"
              }
            }
          ]
        },
        {
          "type": "STRING",
          "value": "}}"
        }
      ]
    },
    "_block_end": {
      "type": "ALIAS",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "{{"
          },
          {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": "/"
              },
              {
                "type": "SYMBOL",
                "name": "_identifier"
              }
            ]
          },
          {
            "type": "STRING",
            "value": "}}"
          }
        ]
      },
      "named": false,
      "value": "/block"
    },
    "handleblock": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_block_start"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "_content"
          }
        },
        {
          "type": "SYMBOL",
          "name": "_block_end"
        }
      ]
    },
    "_macro_start": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "{{"
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "*"
            },
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "_identifier"
                },
                {
                  "type": "SYMBOL",
                  "name": "_expression"
                }
              ]
            }
          ]
        },
        {
          "type": "STRING",
          "value": "}}"
        }
      ]
    },
    "macro": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_macro_start"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "_content"
          }
        },
        {
          "type": "SYMBOL",
          "name": "_block_end"
        }
      ]
    },
    "_partial_start": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "{{"
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": ">"
            },
            {
              "type": "SYMBOL",
              "name": "_expression"
            }
          ]
        },
        {
          "type": "STRING",
          "value": "}}"
        }
      ]
    },
    "partial": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_partial_start"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "_content"
          }
        },
        {
          "type": "SYMBOL",
          "name": "_block_end"
        }
      ]
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}
